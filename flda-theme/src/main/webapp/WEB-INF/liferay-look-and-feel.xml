<?xml version="1.0"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 6.2.0//EN" "http://www.liferay.com/dtd/liferay-look-and-feel_6_2_0.dtd">

<look-and-feel>
	<compatibility>
		<version>6.2.5+</version>
	</compatibility>
	<theme id="flda" name="flda-theme">
		<settings>
			<setting configurable="true" key="facebook" value="" />
			<setting configurable="true" key="twitter" value="" />
			<setting configurable="true" key="youtube" value="" />
		</settings>
		<color-scheme id="01" name="Default">
			<default-cs>true</default-cs>
			<css-class>default</css-class>
		</color-scheme>
		<color-scheme id="02" name="dark">
			<css-class>dark</css-class>
		</color-scheme>
	</theme>	
</look-and-feel>
